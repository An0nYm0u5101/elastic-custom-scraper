              "label_content": [
                "\u5173\u7cfb\u7684\u7279\u5f81",
                "\u8bb8\u53ef\u8bc1\u300b",
                "\u8ba4\u5b9a\u53cc\u65b9\u52b3\u52a8\u5173\u7cfb\u89e3\u9664\u7684\u65f6\u95f4",
                "\u5e76\u672a\u660e\u786e\u53cc\u65b9\u52b3\u52a8\u5173\u7cfb\u89e3\u9664\u7684\u65f6\u95f4",
                "\u53cc\u65b9\u7ea6\u5b9a\u7684\u8fdd\u7ea6\u91d1",
                "\u5c5e\u4e8e\u4e0a\u8ff0\u60c5\u5f62",
                "\u6263\u9664\u8fdd\u7ea6\u91d16000\u7f8e\u5143",
                "\u51fa\u5177\u89e3\u9664\u6216\u8005\u7ec8\u6b62\u52b3\u52a8\u5408\u540c\u7684\u8bc1\u660e",
                "\u7ec8\u6b62\u52b3\u52a8\u5408\u540c\u7684\u8bc1\u660e",
                "\u5e94\u5f53\u627f\u62c5\u8d54\u507f",
                "\u95ee\u9898",
                "\u8fdb\u5165\u8bc9\u8bbc\u7684\u65f6\u95f4\u7b49\u56e0\u7d20",
                "\u51fa\u5177\u79bb\u804c\u8bc1\u660e",
                "\u4e0d\u6e05"
              ],
              "labels": [
                "\u5173\u7cfb",
                "\u8bb8\u53ef\u8bc1",
                "\u8ba4\u5b9a\u52b3\u52a8\u5173\u7cfb\u89e3\u9664",
                "\u5e76\u672a\u52b3\u52a8\u5173\u7cfb",
                "\u53cc\u65b9\u7ea6\u5b9a\u8fdd\u7ea6\u91d1",
                "\u5c5e\u4e8e\u60c5\u5f62",
                "\u6263\u9664",
                "\u51fa\u5177\u89e3\u9664\u8bc1\u660e",
                "\u7ec8\u6b62\u52b3\u52a8\u5408\u540c",
                "\u5e94\u5f53\u627f\u62c5\u8d54\u507f",
                "\u95ee\u9898",
                "\u8fdb\u5165",
