le-system, BlinkMacSystemFont, 'SF Pro Display', 'SF Pro Text','PingFang SC','STHeiti','Helvetica Neue',Helvetica,'Microsoft yahei','\u5fae\u8f6f\u96c5\u9ed1',Arial,sans-serif; color: #333333; font-size: 16px; line-height: 1.4em; font-weight: 400; letter-spacing: 0; -webkit-text-size-adjust: none; -webkit-font-smoothing: antialiased; text-rendering: optimizelegibility; background-color: #ffffff; }\n            .mobile img { display: block; background-position: 50% 0; background-repeat: no-repeat; background-size: 100% 100%; }\n            .mobile img.more { display: inline-block; margin: 0; }\n            .mobile a { color: #0070c9; cursor: pointer; -webkit-tap-highlight-color: rgba(0, 0, 0, 0.3); }\n            .mobile .dark a { color: #555555; }\n            .mobile a:hover { text-decoration: underline; }\n            .mobile div.body { width: 100% !important; max-width: 414px !important; max-height: none !important; float: none !important; overflow: hidden; position: relative; margin: 0 auto; background-color: #ffffff; }\n            .mobile-bottom { background-color: #f2f2f2; }\n            .mobile-bottom div.body { padding-top: 0; background-color: #f2f2f2; }\n            .mobile-bottom div.body p { padding: 0; }\n            .mobile .divider { border-top: 1px solid #d6d6d6; margin: 3px 0 5px; }\n            .mobile .divider.divider-footer { margin: 0; }\n            .mobile .m-apple-logo { margin-bottom: 0; margin-left: 6.25%; text-align: left; }\n            .mobile .m-apple-logo img { margin: 0; }\n            .mobile .m-center, .mobile .m-center img { margin: 0 auto; text-align: center;}\n            .mobile .m-copy { text-align: center !important; line-height: 1.45em; font-size: 17px; }\n            .mobile .m-hero-copy { text-align: center !important; line-height: 1.45em; font-size: 19px; }\n            .mobile .m-head-wrapper { margin: 0 auto; padding-top: 15px;}\n            .mobile .m-content-hero { max-width: 340px; margin: 0 auto; text-align: left;}\n            .mobile .m-content-body { max-width: 340px; margin: 0 auto; text-align: left;}\n            .mobile .m-storeapp img { margin: 0 auto}\n            .mobile .m-hero-section { margin-bottom: 66px; }\n            .mobile .m-hero-section .m-content-hero { padding-top: 0px; }\n            .mobile .m-hero-section .m-content-hero .m-hero-headline { padding-bottom: 23px; padding-top: 6px;}\n            .mobile .m-hero-section .m-hero-cta-button { padding-top: 13px; }\n            .mobile .m-hero-section .m-ipad-hero-image-container img { width: 320px; }\n            .mobile .m-hero-section .m-computer-title {padding: 40px 0 12px;}\n            .mobile .m-hero-section .m-computer-title img { width: 258px; }\n            .mobile .m-hero-section .m-ipad-copy { letter-spacing: -0.02em; padding: 0 22px; }\n            .mobile .m-hero-section .m-hero-cta { padding-top: 20px; letter-spacing: -0.02em; }\n            .mobile .m-hero-section .m-hero-cta-last { padding-bottom: 26px; }\n\n            .mobile .m-trade-in-section { margin-bottom: 28px; padding: 52px 0; background-color: #fafafa;}\n            .mobile .m-trade-in-section .m-content-trade { padding-top: 0; }\n            .mobile .m-trade-in-section .m-trade-headline { padding-bottom: 18px; }\n            .mobile .m-trade-in-section .m-trade-copy { letter-spacing: -0.02em; padding: 0px 0px 0px 10px; }\n            .mobile .m-trade-in-section .m-trade-cta { padding-top: 20px; padding-bottom: 50px; letter-spacing: -0.02em; }\n            .mobile .m-buystrip {background-color: #fff; width: 100% !important; max-width: 414px !important; margin: 0 auto;}\n\n            .mobile .m-buystrip .m-buystrip-icon-asa  {width: 40px; height: 86px; }\n            \n\n            .mobile .m-buystrip .m-buystrip-icon-delivery  {width: 40px; height: 86px; }\n            \n\n            .mobile .m-buystrip .m-buystrip-icon-visit  { width: 40px; height: 86px; }\n            \n\n            .mobile .m-buystrip .m-buystrip-tile { padding: 0 26px; margin: 0 auto; }\n            .mobile .m-buystrip .m-buystrip-subhead { padding-bottom: 6px; }\n            .mobile .m-buystrip .m-buystrip-copy { font-size: 14px; }\n            .mobile .m-buystrip .m-buystrip-delivery { padding-top: 17px;}\n            .mobile .m-buystrip .m-buystrip-delivery .m-buystrip-copy { padding-bottom: 11px; }\n            .mobile .m-buystrip .m-buystrip-delivery .m-buystrip-subhead { position: relative; left: -1px; }\n\n            .mobile .m-buystrip .m-buystrip-finance { padding-top: 34px; }\n            .mobile .m-buystrip .m-buystrip-finance .m-buystrip-icon { padding-left: 2px;}\n            .mobile .m-buystrip .m-buystrip-finance .m-buystrip-subhead { position: relative; left: -1px; }\n            .mobile .m-buystrip .m-buystrip-finance .m-buystrip-copy { padding-bottom: 12px; }\n\n            .mobile .m-buystrip .m-buystrip-app { padding-top: 36px; padding-bottom: 56px; }\n            .mobile .m-buystrip .m-buystrip-app .m-buystrip-subhead { padding-right: 1px; padding-bottom: 7px; position: relative; left: -1px; }\n            .mobile .m-buystrip .m-buystrip-app .m-buystrip-copy { line-height: 1.45em; padding-bottom: 9px; }\n            .mobile .m-buystrip .m-buystrip-app .m-buystrip-copy { line-height: 1.45em; padding-bottom: 16px; }\n            .mobile .m-buystrip .divider { width: 90%; margin: 0 auto; }\n            .mobile .m-trade-image { width: 100%; height: 166px; }\n\n            /* mobile footer styles */\n            .mobile .aapl-footer { color: #888888; padding: 2px 6.25% 0; background: #f2f2f2; padding-top: 4px;}\n            .mobile .aapl-footer a { color: #888888; text-decoration: none; white-space: nowrap;}\n            .mobile .aapl-lowbrow { font-size: 12px; padding: 5px 0  6px; text-align: center; }\n            .mobile .aapl-lowbrow span { padding: 0 6px; }\n            .mobile .aapl-lowbrow span.format-detection { padding: 0; }\n            .mobile .aapl-legal { font-size: 11px; line-height: 16px; padding: 5px 0 29px 1px; text-align: left; }\n            .mobile .aapl-legal .sosumi { line-height: 1.67; padding-bottom: 17px; }\n            .mobile .aapl-legal a { color: #555555; }\n            .mobile .aapl-legal-item { padding-bottom: 6px; }\n            .mobile .aapl-legal span.vert { padding: 0 6px; color: #d6d6d6; }\n            .mobile .aapl-footer .subtle-link { color: #888888; padding: 0; }\n            .mobile .aapl-footer .subtle-link { color: #888888; padding: 0; margin: 0; display: inline; }\n            .mobile .aapl-footer .subtle-link a { color: #888888; }\n            .mobile .vertical-spacer { color: #d6d6d6; }\n            .mobile .rag br { display: none; }\n            .mobile .m-app-section {margin-bottom: 7px; text-align: center;}\n            .mobile .m-iphone-section { background-color: #fafafa; margin-bottom: 7px;}\n            .mobile .m-preorder-section { padding-bottom: 44px; }\n            /*dm specific*/\n        }\n\n    /* iphone 6 plus */\n    /* iphone 6*/\n    /* iphone 5*/\n    @media only screen and (max-device-width: 320px) { .mobile .rag { width: initial; }\n        .mobile .rag br { display: block;; }\n        .m-keyboard-position { margin-left: -47px; }\n    }\n\n    .gmailfix {\n        display:none;\n        display:none!important;\n    }\n\n    </style>\n\n    <!--[if gte mso 9]>\n    <style type=\"text/css\">\n    table, td, div, p { font-family: BlinkMacSystemFont, 'SF Pro Display', 'SF Pro Text','PingFang SC','STHeiti','Helvetica Neue',Helvetica,'Microsoft yahei','\u5fae\u8f6f\u96c5\u9ed1',Arial,sans-serif !important; line-height: normal!important;\n\n}\n\n\n</style>\n<![endif]-->\n<!--[if lte mso 7]>\n<style type=\"text/css\">\ntable, td, div, p { font-family: BlinkMacSystemFont, 'SF Pro Display', 'SF Pro Text','PingFang SC','STHeiti','Helvetica Neue',Helvetica,'Microsoft yahei','\u5fae\u8f6f\u96c5\u9ed1',Arial,sans-serif !important; line-height: normal !important; }\n</style>\n<![endif]-->\n\n</head>\n\n<body st